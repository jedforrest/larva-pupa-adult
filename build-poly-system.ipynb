    "# load data\n",
    "lpa_data = []\n",
    "num_lines = 4\n",
    "for line in open('lpa-data.txt'):\n",
    "    i, L, P, A = line.split()\n",
    "    i = int(i)\n",
    "    L = float(L)\n",
    "    P = float(P)\n",
    "    A = float(A)\n",
    "    print(i, L, P, A)\n",
    "    lpa_data.append((i, L, P, A))\n",
    "    if len(lpa_data) == num_lines:\n",
    "        break"
